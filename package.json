{
  "name": "Watson Assistant with Cloudant & Web Starter",
  "description": "A simple Node.js based web app which shows how to use the Conversation API to recognize user intents.",
  "version": "0.1.1",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-unit && npm run test-integration-runner",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmguillo/assistant-cloudant-webstarter.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@cloudant/cloudant": "^2.1.0",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "cloudant": "^1.10.0",
    "dotenv": "^2.0.0",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "nano-seconds": "^1.2.2",
    "nanoseconds": "^1.0.1",
    "path": "^0.12.7",
    "watson-developer-cloud": "^2.42.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.13",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": ">= 6.9.x",
    "npm": "> 3.10.x"
  }
}
